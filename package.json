{
  "name": "can-component",
  "version": "4.0.0-pre.24",
  "description": "Custom elements for CanJS",
  "homepage": "https://canjs.com/doc/can-component.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-component.git"
  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "http://bitovi.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postpublish": "git push --tags && git push",
    "testee": "testee test/test.html --browsers firefox",
    "test": "npm run detect-cycle && npm run jshint && npm run testee",
    "jshint": "jshint ./*.js --config",
    "release:pre": "npm version prerelease && npm publish --tag=pre",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "develop": "done-serve --static --develop --port 8080",
    "detect-cycle": "detect-cyclic-packages --ignore done-serve"
  },
  "main": "can-component",
  "keywords": [
    "canjs",
    "canjs-plugin",
    "donejs"
  ],
  "system": {
    "configDependencies": [
      "live-reload"
    ]
  },
  "dependencies": {
    "can-assign": "^1.1.1",
    "can-construct": "^3.2.0",
    "can-control": "git://github.com/canjs/can-control.git#magneto",
    "can-dom-events": "git://github.com/canjs/can-dom-events.git#magneto",
    "can-dom-mutate": "git://github.com/canjs/can-dom-mutate.git#four",
    "can-globals": "<2.0.0",
    "can-log": "^0.1.2",
    "can-namespace": "1.0.0",
    "can-observation": "^4.0.0-pre.24",
    "can-queues": "<2.0.0",
    "can-reflect": "^1.6.0",
    "can-simple-map": "^4.0.0-pre.15",
    "can-simple-observable": "^2.0.0-pre.27",
    "can-stache-bindings": "git://github.com/canjs/can-stache-bindings.git#magneto",
    "can-stache-key": "^1.0.0-pre.15",
    "can-symbol": "^1.4.1",
    "can-util": "^3.9.5",
    "can-view-callbacks": "git://github.com/canjs/can-view-callbacks.git#magneto",
    "can-view-model": "git://github.com/canjs/can-view-model.git#magneto",
    "can-view-nodelist": "git://github.com/canjs/can-view-nodelist.git#magneto",
    "can-view-scope": "^4.0.0-pre.45"
  },
  "devDependencies": {
    "can-define": "^2.0.0-pre.24",
    "can-dom-data-state": "^0.2.0",
    "can-observe": "^2.0.0-pre.21",
    "can-stache": "git://github.com/canjs/can-stache.git#magneto",
    "can-vdom": "^3.1.0",
    "detect-cyclic-packages": "^1.1.0",
    "done-serve": "^1.3.0",
    "jshint": "^2.9.1",
    "steal": "^1.0.0",
    "steal-qunit": "^1.0.0",
    "steal-tools": "^1.0.1",
    "testee": "^0.7.0"
  }
}
